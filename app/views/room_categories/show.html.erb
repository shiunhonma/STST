<p id="notice"><%= notice %></p>

<p>
  <strong>Category name:</strong>
  <%= @room_category.category_name %>
</p>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @room.each do |room| %>
      <tr>

        <td><%= room.room_name %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'Edit', edit_room_category_path(@room_category) %> |
<%= link_to 'Back', room_categories_path %>
